<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
    <param-name>db</param-name>
    <param-value>db</param-value>
  </context-param>


  <servlet>
    <servlet-name>loadDatabase</servlet-name>
    <servlet-class>com.sang.api.loadDatabase</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>printDatabaseInfo</servlet-name>
    <servlet-class>com.sang.api.printDatabaseInfo</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.sang.api.register</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.sang.api.login</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>getPatientInfo</servlet-name>
    <servlet-class>com.sang.api.getPatientInfo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getDoctorInfo</servlet-name>
    <servlet-class>com.sang.api.getDoctorInfo</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>com.sang.api.logout</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>doctorsList</servlet-name>
    <servlet-class>com.sang.api.doctorsList</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>orderDoctor</servlet-name>
    <servlet-class>com.sang.api.orderDoctor</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>cancelOrder</servlet-name>
    <servlet-class>com.sang.api.cancelOrder</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>diagnose</servlet-name>
    <servlet-class>com.sang.api.diagnose</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>listOrders</servlet-name>
    <servlet-class>com.sang.api.listOrders</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>listPatients</servlet-name>
    <servlet-class>com.sang.api.listPatients</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>setInfo</servlet-name>
    <servlet-class>com.sang.api.setInfo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>monitor</servlet-name>
    <servlet-class>com.sang.api.monitor</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>pushNews</servlet-name>
    <servlet-class>com.sang.api.pushNews</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>clearAllPatients</servlet-name>
    <servlet-class>com.sang.api.clearAllPatients</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>loadDatabase</servlet-name>
    <url-pattern>/loadDatabase</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>printDatabaseInfo</servlet-name>
    <url-pattern>/printDatabaseInfo</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>getPatientInfo</servlet-name>
    <url-pattern>/getPatientInfo</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>getDoctorInfo</servlet-name>
    <url-pattern>/getDoctorInfo</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>doctorsList</servlet-name>
    <url-pattern>/doctorsList</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>orderDoctor</servlet-name>
    <url-pattern>/orderDoctor</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>cancelOrder</servlet-name>
    <url-pattern>/cancelOrder</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>diagnose</servlet-name>
    <url-pattern>/diagnose</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>listOrders</servlet-name>
    <url-pattern>/listOrders</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>listPatients</servlet-name>
    <url-pattern>/listPatients</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>setInfo</servlet-name>
    <url-pattern>/setInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>monitor</servlet-name>
    <url-pattern>/monitor</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>pushNews</servlet-name>
    <url-pattern>/pushNews</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>clearAllPatients</servlet-name>
    <url-pattern>/clearAllPatients</url-pattern>
  </servlet-mapping>


  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>
